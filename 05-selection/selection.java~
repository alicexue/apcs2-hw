import java.util.*;
import java.io.*;

public class selection {
    int[] L;

    public void select (ArrayList<Integer> L, int SI, int EI) {
	int p;
	int[] D = new int[L.length];
	if (EI<E.length) {
	    int i=EI
		for (;i<E.length;i++) {
		D[i] = E[i];
	    }
	    D[i] = L[SI];
	    p = L[SI];
	}
	while (SI<EI) {
	    int q = L[SI];
	    if (q<p) {
		D[SI]=q;
		SI++;
	    } else {
		D[EI]=q;
		SI--;
	    }
	}
    }
}

public static void main (String[] args) {
    selection s = new selection();
    int [] a = {2, 5, 3, 1, 35, 4, 54, 6, 7, 15};
    s.select(a, 2, 7);
	
	